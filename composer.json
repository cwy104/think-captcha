{
  "name": "cwy104/think-captcha",
  "description": "captcha package for thinkphp，前后端分离，api接口调用",
  "authors": [
    {
      "name": "cwy104",
      "email": "cwy104@vip.qq.com"
    }
  ],
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1.0",
    "topthink/framework": "^6.0.0"
  },
  "autoload": {
    "psr-4": {
      "cwy104\\captcha\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "extra": {
    "think": {
      "services": [
        "cwy104\\captcha\\CaptchaService"
      ],
      "config": {
        "captcha": "src/config.php"
      }
    }
  }
}
